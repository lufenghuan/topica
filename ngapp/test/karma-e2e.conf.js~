var sharedConfig = require('./karma-shared.conf');

module.exports = function(config) {
  var conf = sharedConfig();

  conf.files = conf.files.concat([
    //test files
    'test/e2e/*.js',
  ]);

  conf.proxies = {
    '/': 'http://localhost:9876/'
  };

  conf.frameworks = ['ng-scenario'];

  config.set(conf);
};
